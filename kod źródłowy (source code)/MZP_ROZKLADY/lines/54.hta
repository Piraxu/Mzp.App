             (screen.availHeight - wysokosc) / 2
         );
     }

     function enforceWindowSize() {
         var szerokosc = 319;
         var wysokosc = 586;

         if (window.outerWidth !== szerokosc || window.outerHeight !== wysokosc) {
             window.resizeTo(szerokosc, wysokosc);
             window.moveTo(
                 (screen.availWidth - szerokosc) / 2,
                 (screen.availHeight - wysokosc) / 2
             );
         }
     }

