<script language="javascript">
function runFile(p) {
    var sh = new ActiveXObject("WScript.Shell");
    sh.Run(p);
}



function runTimetable(relativeJsonPath) {
    var sh = new ActiveXObject("WScript.Shell");

    // katalog, w kt√≥rym znajduje siƒô 9.hta
    var basePath = unescape(location.pathname)
        .replace(/\//g,"\\")
        .replace(/^\\([A-Za-z]:)/,"$1")
        .replace(/[^\\]*$/,"");

    // pe≈Çna ≈õcie≈ºka do timetable.hta
    var timetablePath = basePath + "timetable.hta";

    // pe≈Çna ≈õcie≈ºka do pliku JSON (na podstawie tego, co podasz w onclick)
    var jsonPath = basePath + relativeJsonPath;

    // sk≈Çadamy ko≈ÑcowƒÖ komendƒô
    var cmd = 'mshta.exe "' + timetablePath + '?plik=' + jsonPath + '"';

    // odpalamy
    sh.Run(cmd, 1, false);
}

</script>


